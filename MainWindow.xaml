<Window x:Class="u5_QuadraticExtended_CouperEbbsPicken.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:u5_QuadraticExtended_CouperEbbsPicken"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" MaxHeight="500" MaxWidth="650" MinHeight="500" MinWidth="650">
    <Canvas x:Name="canvas">
        <TextBox x:Name="txt_AValue" Text="type_'a'_value_here" Canvas.Left="10" Canvas.Top="21" Height="17" Width="106"></TextBox>
        <TextBox x:Name="txt_BValue" Text="type_'b'_value_here" Canvas.Left="10" Canvas.Top="43" Height="17" Width="106"/>
        <TextBox x:Name="txt_CValue" Text="type_'a'_value_here" Canvas.Top="65" Height="17" Width="106" Canvas.Left="10"/>
        <Button x:Name="btn_run" Height="16" Width="106" Canvas.Left="10" Content="Run" Click="btn_run_Click"></Button>
        <Label x:Name="lbl_Output" Content="Output" Canvas.Left="10" Canvas.Top="87" Width="106" Height="24"></Label>
    </Canvas>
</Window>
